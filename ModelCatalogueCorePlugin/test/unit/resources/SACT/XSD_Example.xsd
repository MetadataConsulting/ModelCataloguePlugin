<?xml version="1.0"?>
<xs:schema xmlns="http://www.datadictionary.nhs.uk/messages/SACT-v2-0" xmlns:SACT="http://www.datadictionary.nhs.uk/messages/SACT-v2-0" xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.datadictionary.nhs.uk/messages/SACT-v2-0" elementFormDefault="unqualified" attributeFormDefault="unqualified" blockDefault="#all" version="2-0">
    <xs:complexType name="TS.GB-en-NHS.Date">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of a date of the form: yyyy-mm-dd.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="TS">
                <xs:attribute name="value" use="required">
                    <xs:simpleType>
                        <xs:restriction base="ts">
                            <xs:pattern value="(19|20)\d\d(0[1-9]|1[012])(0[1-9]|[12][0-9]|3[01])"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="PN.NHS.Internal" abstract="true" mixed="true">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of a name of the form: {prefix} {given} {given} {family}.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ANY">
                <xs:sequence>
                    <xs:element name="prefix" type="en.prefix" minOccurs="0"/>
                    <xs:element name="given" type="en.given" minOccurs="0" maxOccurs="2"/>
                    <xs:element name="family" type="en.family" minOccurs="0"/>
                </xs:sequence>
                <xs:attribute name="use" type="set_cs_EntityNameUse" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!--ST Restriction to support Dictionary Dates-->
    <xs:simpleType name="ST.GB-en-NHS.StringType1">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of dictionary dates.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:pattern value="(19|20)\d\d-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])"/>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support Dictionary Boolean-->
    <xs:simpleType name="ST.GB-en-NHS.StringType2">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of a boolean with value 'Y' or 'N'.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:enumeration value="Y"/>
            <xs:enumeration value="N"/>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support alphanumeric only-->
    <xs:simpleType name="ST.GB-en.NHS.StringType3">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of an alphanumeric string.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:pattern value="[a-zA-Z0-9]*"/>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support Dates-->
    <xs:simpleType name="ST.GB-en-NHS.StringType4">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of date and time.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:pattern value="(19|20)[0-9][0-9]-(0[1-9]|1[012])-(0[1-9]|[12][0-9]|3[01])T(0[0-9]|1[0-9]|2[0-3]):([0-5][0-9]):([0-5][0-9])"/>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support II Type 2 as String-->
    <xs:simpleType name="ST.GB-en-NHS.StringType5">
        <xs:annotation>
            <xs:documentation>This data type supports the sending UUID as string with in element.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:pattern value="[0-9A-F]{8}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{12}">
                <xs:annotation>
                    <xs:documentation>
                        Pattern facet to allow upper case hex in the uuid in field element.
                    </xs:documentation>
                </xs:annotation>
            </xs:pattern>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support II Type 5 NHS Number as String-->
    <xs:simpleType name="ST.GB-en-NHS.StringType6">
        <xs:annotation>
            <xs:documentation>This data type supports the sending of NHS Number as string.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:length value="10"/>
            <xs:pattern value="[0-9]{10}"/>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support II Type 6 as String-->
    <xs:simpleType name="ST.GB-en-NHS.StringType7">
        <xs:annotation>
            <xs:documentation>This data type supports the sending of Radiological Accession Number content as string.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:minLength value="1"/>
            <xs:maxLength value="20"/>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support II Local Patient Identifier as String-->
    <xs:simpleType name="ST.GB-en-NHS.StringType8">
        <xs:annotation>
            <xs:documentation>This data type supports the sending of Local Patient Id as string.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:minLength value="1"/>
            <xs:maxLength value="10"/>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support II Type 7 Referrer Code as String-->
    <xs:simpleType name="ST.GB-en-NHS.StringType9">
        <xs:annotation>
            <xs:documentation>This data type supports the sending of referrer code as string.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:length value="8"/>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support II Type 4 as String-->
    <xs:simpleType name="ST.GB-en-NHS.StringType10">
        <xs:annotation>
            <xs:documentation>This data type supports the sending of Organisation data as derived from ODS as alphanumeric string.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:minLength value="3"/>
            <xs:maxLength value="12"/>
            <xs:pattern value="[A-Z0-9]*"/>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support the representation time-->
    <xs:simpleType name="ST.GB-en-NHS.StringType11">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of a time.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:pattern value="(0[0-9]|1[0-9]|2[0-3]):([0-5][0-9]):([0-5][0-9])"/>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support CodedValueType7 OPCS Code as String-->
    <xs:simpleType name="ST.GB-en-NHS.StringType12">
        <xs:annotation>
            <xs:documentation>This data type supports the sending of OPCS code as string.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:length value="4"/>
            <xs:pattern value="[a-zA-Z0-9]*"/>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support CodedValueType5 ICDO Code as String-->
    <xs:simpleType name="ST.GB-en-NHS.StringType13">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of an ICDO code as string.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:minLength value="5"/>
            <xs:maxLength value="7"/>
            <xs:pattern value="[a-zA-Z0-9]*"/>
        </xs:restriction>
    </xs:simpleType>
    <!--ST Restriction to support CodedValueType6 ICD10 (ICD) Code as String-->
    <xs:simpleType name="ST.GB-en-NHS.StringType14">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of ICD10 (ICD) content as string.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="st">
            <xs:minLength value="4"/>
            <xs:maxLength value="6"/>
            <xs:pattern value="[a-zA-Z0-9]*"/>
        </xs:restriction>
    </xs:simpleType>
    <!--PN Restriction to support Family name only-->
    <xs:complexType name="PN.GB-en-NHS.PersonNameType4">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of a family name.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="prefix" type="en.prefix" minOccurs="0" maxOccurs="0"/>
            <xs:element name="given" type="en.given" minOccurs="0" maxOccurs="0"/>
            <xs:element name="family">
                <xs:simpleType>
                    <xs:restriction base="st">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="35"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
        <xs:attribute name="use" type="set_cs_EntityNameUse" use="prohibited"/>
    </xs:complexType>
    <!--PN Restriction to support Given name only-->
    <xs:complexType name="PN.GB-en-NHS.PersonNameType5">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of a given name.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="prefix" type="en.prefix" minOccurs="0" maxOccurs="0"/>
            <xs:element name="given">
                <xs:simpleType>
                    <xs:restriction base="st">
                        <xs:minLength value="1"/>
                        <xs:maxLength value="35"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="family" type="en.family" minOccurs="0" maxOccurs="0"/>
        </xs:sequence>
        <xs:attribute name="use" type="set_cs_EntityNameUse" use="prohibited"/>
    </xs:complexType>
    <!--II Restriction to support SDS Org-->
    <xs:complexType name="II.GB-en-NHS.IdentifierType4">
        <xs:annotation>
            <xs:documentation>This data type supports the sending of Organisation data as derived from ODS.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="II">
                <xs:attribute name="root" type="uid" use="prohibited" fixed="2.16.840.1.113883.2.1.3.2.4.19.1"/>
                <xs:attribute name="extension" use="required">
                    <xs:simpleType>
                        <xs:restriction base="st">
                            <xs:minLength value="3"/>
                            <xs:maxLength value="12"/>
                            <xs:pattern value="[A-Z0-9]*"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="assigningAuthorityName" type="st" use="prohibited"/>
                <xs:attribute name="displayable" type="bl" use="prohibited"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="II.GB-en-NHS.IdentifierType2">
        <xs:annotation>
            <xs:documentation>This data type supports the sending of DCE UUID</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="II.NPfIT.uuid.required">
                <xs:attribute name="root" type="II.NPfIT.root.uuid" use="required"/>
                <xs:attribute name="extension" type="st" use="prohibited"/>
                <xs:attribute name="nullFlavor" type="cs_NullFlavor" use="prohibited"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="II.NPfIT.uuid.required">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of a UUID.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="II">
                <xs:attribute name="root" type="II.NPfIT.root.uuid" use="optional"/>
                <xs:attribute name="extension" type="st" use="prohibited"/>
                <xs:attribute name="nullFlavor" type="cs_NullFlavor" use="optional"/>
                <xs:attribute name="updateMode" type="cs_UpdateMode" use="prohibited"/>
                <xs:attribute name="assigningAuthorityName" type="st" use="prohibited"/>
                <xs:attribute name="displayable" type="bl" use="prohibited"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:simpleType name="II.NPfIT.root.uuid">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of a UUID.</xs:documentation>
        </xs:annotation>
        <xs:restriction base="uid">
            <xs:pattern value="[0-9A-F]{8}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{4}-[0-9A-F]{12}">
                <xs:annotation>
                    <xs:documentation>
                        Pattern facet to allow upper case hex in the uuid in root of II data type
                    </xs:documentation>
                </xs:annotation>
            </xs:pattern>
        </xs:restriction>
    </xs:simpleType>
    <!--Radiological Accession Number-->
    <!--II Restriction to support NHS Number-->
    <xs:complexType name="II.GB-en-NHS.IdentifierType5">
        <xs:annotation>
            <xs:documentation>This data type supports the use of NHS Number content</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="II">
                <xs:attribute name="root" type="uid" use="prohibited" fixed="2.16.840.1.113883.2.1.4.1"/>
                <xs:attribute name="extension" use="required">
                    <xs:simpleType>
                        <xs:restriction base="st">
                            <xs:length value="10"/>
                            <xs:pattern value="[0-9]{10}"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="assigningAuthorityName" type="st" use="prohibited"/>
                <xs:attribute name="displayable" type="bl" use="prohibited"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="II.GB-en-NHS.IdentifierType6">
        <xs:annotation>
            <xs:documentation>This data type supports the use of Radiological Accession Number content.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="II">
                <xs:attribute name="root" type="uid" use="prohibited" fixed="2.16.840.1.113883.2.1.3.2.4.22"/>
                <xs:attribute name="extension" use="required">
                    <xs:simpleType>
                        <xs:restriction base="st">
                            <xs:minLength value="1"/>
                            <xs:maxLength value="20"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="assigningAuthorityName" type="st" use="prohibited"/>
                <xs:attribute name="displayable" type="bl" use="prohibited"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <!--II Restriction to support Referrer Code-->
    <xs:complexType name="II.GB-en-NHS.IdentifierType7">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of a referrer code.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="II">
                <xs:attribute name="root" type="uid" use="prohibited" fixed="2.16.840.1.113883.2.1.3.2.4.16.60"/>
                <xs:attribute name="extension" use="required">
                    <xs:simpleType>
                        <xs:restriction base="st">
                            <xs:length value="8"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="assigningAuthorityName" type="st" use="prohibited"/>
                <xs:attribute name="displayable" type="bl" use="prohibited"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <!--Type to support Local Person Identifier-->
    <xs:complexType name="II.GB-en-NHS.IdentifierType8">
        <xs:complexContent>
            <xs:restriction base="II">
                <xs:attribute name="root" type="uid" use="prohibited" fixed="2.16.840.1.113883.2.1.3.2.4.18.24"/>
                <xs:attribute name="extension" type="st" use="required"/>
                <xs:attribute name="assigningAuthorityName" type="st" use="prohibited"/>
                <xs:attribute name="displayable" type="bl" use="prohibited"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <!--AD Restriction to support postalCode only-->
    <xs:complexType name="AD.GB-en-NHS.AddressType1">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of an address represented with a postcode attribute.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ANY">
                <xs:sequence>
                    <xs:element name="postalCode" type="postalcode"/>
                </xs:sequence>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="AD.NHS.Internal" abstract="true" mixed="true">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of an address.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:extension base="ANY">
                <xs:sequence>
                    <xs:element name="postalCode" type="ADXP" minOccurs="0"/>
                    <xs:element name="streetAddressLine" type="ADXP" minOccurs="0" maxOccurs="5"/>
                </xs:sequence>
                <xs:attribute name="use" type="set_cs_PostalAddressUse" use="optional"/>
            </xs:extension>
        </xs:complexContent>
    </xs:complexType>
    <!--AD Restriction to support 5 street address lines only-->
    <xs:complexType name="AD.GB-en-NHS.AddressType4" mixed="true">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of a structured or unstructured address.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
            <xs:element name="postalCode" type="ADXP" minOccurs="0" maxOccurs="0"/>
            <xs:choice>
                <xs:element name="UnstructuredAddress">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="streetAddressLine">
                                <xs:simpleType>
                                    <xs:restriction base="st">
                                        <xs:maxLength value="175"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="StructuredAddress">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="streetAddressLine" minOccurs="5" maxOccurs="5">
                                <xs:simpleType>
                                    <xs:restriction base="st">
                                        <xs:maxLength value="35"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:choice>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="AD.NHS.AddressType1" mixed="true">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of an address</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="AD.NHS.Internal">
                <xs:sequence>
                    <xs:element name="postalCode" type="ADXP"/>
                    <xs:element name="streetAddressLine" type="ADXP" minOccurs="0" maxOccurs="0"/>
                </xs:sequence>
                <xs:attribute name="nullFlavor" type="cs_NullFlavor" use="prohibited"/>
                <xs:attribute name="updateMode" type="cs_UpdateMode" use="prohibited"/>
                <xs:attribute name="use" type="set_cs_PostalAddressUse" use="prohibited"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <!--CV Restriction to support Code only-->
    <xs:complexType name="CV.GB-en-NHS.CodedValueType1">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of a code only.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="CV">
                <xs:attribute name="codeSystemName" type="st" use="prohibited"/>
                <xs:attribute name="codeSystemVersion" type="st" use="prohibited"/>
                <xs:attribute name="displayName" type="st" use="prohibited"/>
                <xs:attribute name="code" type="cs" use="required"/>
                <xs:attribute name="codeSystem" type="uid" use="prohibited"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <!--CV Restriction to support NICIP Code-->
    <xs:complexType name="CV.GB-en-NHS.CodedValueType2">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of NICIP content</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="CV">
                <xs:attribute name="codeSystemName" type="st" use="prohibited"/>
                <xs:attribute name="codeSystemVersion" type="st" use="prohibited"/>
                <xs:attribute name="displayName" type="st" use="prohibited"/>
                <xs:attribute name="code" use="required">
                    <xs:simpleType>
                        <xs:restriction base="cs">
                            <xs:minLength value="1"/>
                            <xs:maxLength value="6"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="codeSystem" type="uid" use="prohibited" fixed="2.16.840.1.113883.2.1.3.2.4.21"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <!--CV Restriction to support SNOMED CT-->
    <xs:complexType name="CV.GB-en-NHS.CodedValueType3">
        <xs:annotation>
            <xs:documentation>This type supports SNOMED-CT codes.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="CV">
                <xs:attribute name="codeSystemName" type="st" use="prohibited"/>
                <xs:attribute name="codeSystemVersion" type="st" use="prohibited"/>
                <xs:attribute name="displayName" type="st" use="prohibited"/>
                <xs:attribute name="code" use="required">
                    <xs:simpleType>
                        <xs:restriction base="cs">
                            <xs:minLength value="1"/>
                            <xs:maxLength value="18"/>
                            <xs:pattern value="\d{1,18}"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="codeSystem" type="uid" use="prohibited" fixed="2.16.840.1.113883.2.1.3.2.4.15"/>
                <xs:attribute name="nullFlavor" type="cs_NullFlavor" use="prohibited"/>
                <xs:attribute name="updateMode" type="cs_UpdateMode" use="prohibited"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="CV.GB-en-NHS.CodedValueType4">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of value in the set: 'Y', 'N', 'X'.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="CV">
                <xs:attribute name="codeSystemName" type="st" use="prohibited"/>
                <xs:attribute name="codeSystemVersion" type="st" use="prohibited"/>
                <xs:attribute name="displayName" type="st" use="prohibited"/>
                <xs:attribute name="code" use="required">
                    <xs:simpleType>
                        <xs:restriction base="cs">
                            <xs:enumeration value="Y"/>
                            <xs:enumeration value="N"/>
                            <xs:enumeration value="X"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="codeSystem" type="uid" use="prohibited"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="CV.GB-en-NHS.CodedValueType5">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of ICDO content.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="CV">
                <xs:attribute name="codeSystemName" type="st" use="prohibited"/>
                <xs:attribute name="codeSystemVersion" type="st" use="prohibited"/>
                <xs:attribute name="displayName" type="st" use="prohibited"/>
                <xs:attribute name="code" use="required">
                    <xs:simpleType>
                        <xs:restriction base="cs">
                            <xs:minLength value="5"/>
                            <xs:maxLength value="7"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:attribute>
                <xs:attribute name="codeSystem" type="uid" use="prohibited" fixed="2.16.840.1.113883.2.1.3.2.4.16.69"/>
            </xs:restriction>
        </xs:complexContent>
    </xs:complexType>
    <xs:complexType name="CV.GB-en-NHS.CodedValueType6">
        <xs:annotation>
            <xs:documentation>This data type supports the representation of ICD10 content.</xs:documentation>
        </xs:annotation>
        <xs:complexContent>
            <xs:restriction base="CV">
