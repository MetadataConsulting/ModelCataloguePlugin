<?xml version="1.0" encoding="UTF-8"?>

<headersMap xmlns="model_catalogue_excel_header_mapping"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="model_catalogue_excel_header_mapping model_catalogue_excel_header_mapping.xsd">
    
    <!-- How to use this configuration file: Put the header name e.g. Code in the tag dataElementID.
  This will use the entry under Code as the Data Element ID.
  <dataElementID>Code</dataElementID> -->
    
	<dataClassID>Set</dataClassID>
    <dataClassName>Set Name</dataClassName>
    <dataElementID>Code</dataElementID>
    <dataElementName>Code name</dataElementName> <!-- must occur-->
    <dataTypeName>Data Type</dataTypeName>
    <dataTypeDescription>Format</dataTypeDescription>
    <measurementUnitName>Unit</measurementUnitName>
    <metadata>Code abbreviation</metadata> <!-- can have multiple -->
    <metadata>Reportable</metadata>
    <metadata>Mandatory</metadata>
    <metadata>Suppressed</metadata>
    <metadata>Length</metadata>
    <metadata>Line count</metadata>
    <metadata>Ref Range</metadata>
</headersMap>